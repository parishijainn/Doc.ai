{
  "name": "carezoom-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "node -e \"const fs=require('fs'); fs.rmSync('.next',{recursive:true,force:true}); fs.rmSync('.next-dev',{recursive:true,force:true}); fs.rmSync('node_modules/.cache',{recursive:true,force:true});\"",
    "dev": "NEXT_DIST_DIR=.next-dev WATCHPACK_POLLING=true WATCHPACK_POLLING_INTERVAL=1000 NEXT_TELEMETRY_DISABLED=1 next dev -p 3000",
    "dev:clean": "lsof -ti :3000 | xargs kill -9 2>/dev/null || true && npm run clean && npm run dev",
    "build": "NEXT_DIST_DIR=.next next build",
    "start": "NEXT_DIST_DIR=.next next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@daily-co/daily-js": "^0.64.0",
    "lucide-react": "^0.563.0",
    "maplibre-gl": "^5.7.0",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3"
  }
}
